{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "Infrastructure/obj": true,
        "Infrastructure/bin": true,
        "Application/obj": true,
        "Application/bin": true,
        ".idea": true,
        "Api.Minimal/obj": true,
        "Api.Minimal/bin": true,
        "Api.Report/bin": true,
        "Api.Report/obj": true,
        "Domain/bin": true,
        "Domain/obj": true,
        ".gitattributes": true
    },
    "hide-files.files": [
        "Infrastructure/obj",
        "Infrastructure/bin",
        "Application/obj",
        "Application/bin",
        ".idea",
        "Api.Minimal/obj",
        "Api.Minimal/bin",
        "Api.Report/bin",
        "Api.Report/obj",
        "Domain/bin",
        "Domain/obj",
        ".gitattributes"
    ],
    "cSpell.words": [
        "ASPNETCORE",
        "CORRELATIONIDHEADERNAME",
        "Mediat",
        "Repositorys",
        "Serilog",
        "THCS"
    ],
    "workbench.tree.indent": 10,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "window.autoDetectColorScheme": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontWeight": "300",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.tabs.enabled": true,
    "cSpell.allowCompoundWords": true,
    "cSpell.language": "en,en-US",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "editor.formatOnSave": true,
    "editor.fontLigatures": true,
    "editor.smoothScrolling": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.guides.bracketPairs": "active",
    "editor.scrollbar.horizontal": "hidden",
    "editor.minimap.enabled": false,
    "files.autoSave": "afterDelay",
    "editor.inlineSuggest.enabled": true,
    "editor.cursorBlinking": "expand",
    "debug.toolBarLocation": "docked",
    "debug.openExplorerOnEnd": true,
    "editor.linkedEditing": true,
    "workbench.iconTheme": "vscode-icons",
    "dotnet.navigation.navigateToDecompiledSources": false,
    "dotnet.server.useOmnisharp": true,
    "dotnet.backgroundAnalysis.analyzerDiagnosticsScope": "fullSolution",
    "dotnet.backgroundAnalysis.compilerDiagnosticsScope": "fullSolution",
    "csharp.experimental.debug.hotReload": true,
    "csharp.format.enable": true,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "razor.devmode": true,
    "workbench.colorTheme": "Simple Dark",
    "editor.fontFamily": "'Jetbrains Mono Light', 'Fira Code', 'Cascadia Code', Consolas, 'Courier New', monospace",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font', 'Input Mono'",
    "dotnet.projects.enableAutomaticRestore": false
}